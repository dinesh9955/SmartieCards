<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/sssss"
    >


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rl111"
        android:visibility="visible"

        >
        <include
            layout="@layout/tool_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/main_relative"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/rl111">


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="UselessParent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="10dp">


                <ImageView
                    android:id="@+id/imageView12234"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginRight="40dp"
                    android:contentDescription="TODO"
                    android:src="@drawable/logo"
                    android:visibility="visible"
                    tools:ignore="ContentDescription" />




                <EditText
                    android:id="@+id/login_fname"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:drawableLeft="@drawable/username"
                    android:hint="First Name"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:textColorHint="@color/text_hint"
                    android:textColor="@color/white"
                    android:text=""
                    android:textSize="15dp"
                    android:layout_below="@+id/imageView12234"
                    android:singleLine="true"
                    android:visibility="visible"
                    />



                <EditText
                    android:id="@+id/login_lname"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:drawableLeft="@drawable/username"
                    android:hint="Last Name"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:textColorHint="@color/text_hint"
                    android:textColor="@color/white"
                    android:text=""
                    android:textSize="15dp"
                    android:layout_below="@+id/login_fname"
                    android:singleLine="true"
                    />





                <EditText
                    android:id="@+id/login_username"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:drawableLeft="@drawable/username"
                    android:hint="Username"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:textColorHint="@color/text_hint"
                    android:textColor="@color/white"
                    android:text=""
                    android:textSize="15dp"
                    android:layout_below="@+id/login_lname"
                    android:singleLine="true"
                    />




                <EditText
                    android:id="@+id/login_email"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:drawableLeft="@drawable/email"
                    android:hint="Email"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:textColorHint="@color/text_hint"
                    android:textColor="@color/white"
                    android:text=""
                    android:textSize="15dp"
                    android:layout_below="@+id/login_username"
                    android:singleLine="true"
                    android:inputType="textEmailAddress"
                    />




                <EditText
                    android:id="@+id/login_password"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:drawableLeft="@drawable/pwd"
                    android:hint="Password"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:textColorHint="@color/text_hint"
                    android:textColor="@color/white"
                    android:text=""
                    android:textSize="15dp"
                    android:layout_below="@+id/login_email"
                    android:singleLine="true"
                    android:inputType="textPassword"
                    />


                <EditText
                    android:id="@+id/login_con_password"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:drawableLeft="@drawable/pwd"
                    android:hint="Confirm Password"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:textColorHint="@color/text_hint"
                    android:textColor="@color/white"
                    android:text=""
                    android:textSize="15dp"
                    android:layout_below="@+id/login_password"
                    android:singleLine="true"
                    android:inputType="textPassword"
                    />




                <Button
                    android:id="@+id/button678789789"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/login_con_password"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:text="@string/login_dob"
                    android:textColor="@android:color/white"
                    android:textSize="15dp"
                    android:textStyle="normal"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="left|center_vertical"
                    android:textAllCaps="false"
                    android:drawableLeft="@drawable/dobicon"/>





                <EditText
                    android:id="@+id/login_address"
                    android:layout_width="fill_parent"
                    android:layout_height="90dp"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:hint="Address"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:textColorHint="@color/text_hint"
                    android:textColor="@color/white"
                    android:text=""
                    android:textSize="15dp"
                    android:layout_below="@+id/button678789789"
                    android:gravity="left|top"
                    />



                <EditText
                    android:id="@+id/login_city"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:background="@drawable/fade_green_border_corner_round"
                    android:hint="City"
                    android:padding="10dp"
                    android:drawablePadding="10dp"
                    android:layout_marginTop="10dp"
                    android:textColorHint="@color/text_hint"
                    android:textColor="@color/white"
                    android:text=""
                    android:textSize="15dp"
                    android:layout_below="@+id/login_address"
                    android:gravity="left|center_vertical"
                    android:singleLine="true"
                    />




                <RelativeLayout
                    android:id="@+id/relativeLayout16"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/login_city"
                    android:layout_marginTop="10dp"
                    android:visibility="visible"
            >


                    <ProgressBar
                        android:id="@+id/progressBar3657475"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:visibility="gone"/>

                    <Spinner
                        android:id="@+id/spinner645657"
                        android:layout_width="fill_parent"
                        android:layout_height="45dp"
                        android:background="@drawable/shape_drop_down_normal_round"
                        android:textColor="#000000"
                        android:paddingLeft="0dp"
                        android:drawablePadding="10dp"
                        android:hint="Select"
                        android:textColorHint="@color/white"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_marginTop="0dp"
                        android:popupBackground="@color/green_fade_hover_color"
                        android:spinnerMode="dropdown"
                        android:dropDownWidth="fill_parent"

                        />

                </RelativeLayout>






                <RelativeLayout
                    android:id="@+id/checkbox_accept1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/relativeLayout16"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    >


                    <CheckBox
                        android:id="@+id/checkBox111"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@null"
                        android:scaleX="1.00"
                        android:scaleY="1.00"
                        android:textColor="@android:color/black"
                        android:theme="@style/AppThemeCheckBoxRegister"
                        />


                    <TextView
                        android:id="@+id/textView12sdd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:text="I accept Terms of Service and Privacy Policy."
                        android:layout_toRightOf="@+id/checkBox111"
                        android:layout_marginTop="3dp"
                        android:buttonTint="@color/red"
                        android:textColor="@android:color/white"
                        android:textSize="14dp"
                        />

                </RelativeLayout>




                <Button
                    android:id="@+id/button6546"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/checkbox_accept1"
                    android:background="@drawable/black_selector"
                    android:text="@string/login_signup"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:textStyle="normal"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:textAllCaps="true"/>


            </RelativeLayout>


        </ScrollView>

    </RelativeLayout>


</RelativeLayout>
